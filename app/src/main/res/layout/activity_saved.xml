<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_saved"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.wolf.colorex.saved">

<TextView
    android:layout_width="match_parent"
    android:layout_height="50dp"
    android:layout_centerHorizontal="true"
    android:textSize="28sp"
    android:textStyle="bold"
    android:textColor="#eeffffff"
    android:id="@+id/et1"
    />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:textStyle="bold"
        android:textColor="#eeffffff"
        android:textSize="28sp"
        android:id="@+id/et2"
        android:layout_below="@id/et1"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:textStyle="bold"
        android:textColor="#eeffffff"
        android:layout_centerHorizontal="true"
        android:textSize="28sp"
        android:id="@+id/et3"
        android:layout_below="@id/et2"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:textStyle="bold"
        android:textColor="#eeffffff"
        android:layout_centerHorizontal="true"
        android:textSize="28sp"
        android:id="@+id/et4"
        android:layout_below="@id/et3"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:textStyle="bold"
        android:textColor="#eeffffff"
        android:layout_centerHorizontal="true"
        android:textSize="28sp"
        android:id="@+id/et5"
        android:layout_below="@id/et4"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:textStyle="bold"
        android:textColor="#eeffffff"
        android:layout_centerHorizontal="true"
        android:textSize="28sp"
        android:id="@+id/et6"
        android:layout_below="@id/et5"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:textStyle="bold"
        android:textColor="#eeffffff"
        android:layout_centerHorizontal="true"
        android:textSize="28sp"
        android:id="@+id/et7"
        android:layout_below="@id/et6"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:textStyle="bold"
        android:textColor="#eeffffff"
        android:layout_centerHorizontal="true"
        android:textSize="28sp"
        android:id="@+id/et8"
        android:layout_below="@id/et7"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:textStyle="bold"
        android:textColor="#eeffffff"
        android:layout_centerHorizontal="true"
        android:textSize="28sp"
        android:id="@+id/et9"
        android:layout_below="@id/et8"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:textStyle="bold"
        android:textColor="#eeffffff"
        android:layout_centerHorizontal="true"
        android:textSize="28sp"
        android:id="@+id/et10"
        android:layout_below="@id/et9"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:textStyle="bold"
        android:textColor="#eeffffff"
        android:layout_centerHorizontal="true"
        android:textSize="28sp"
        android:id="@+id/et11"
        android:layout_below="@id/et10"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:textStyle="bold"
        android:textColor="#eeffffff"
        android:layout_centerHorizontal="true"
        android:textSize="28sp"
        android:id="@+id/et12"
        android:layout_below="@id/et11"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:textAlignment="center"
        android:layout_alignParentBottom="true"
        android:id="@+id/text"
        android:textSize="24sp"
        android:textColor="#323232"
        android:text="SAVED COLORS"/>
    <ImageView
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:src="@drawable/left"
        android:layout_alignParentBottom = "true"
        android:id="@+id/imageView"
        android:layout_alignParentLeft="true"
        />
</RelativeLayout>
